<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".PeedViewActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/score_Logo"
                android:layout_width="121dp"
                android:layout_height="63dp"
                android:background="@mipmap/main_logo"
                android:layout_margin="10dp"/>
        </FrameLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_gravity="center">
                    <FrameLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/score_StoreName_EditText"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textColorHint="@color/dark_gray"
                            android:textColor="@color/black"
                            android:text="가게 이름"
                            android:textSize="20dp"
                            android:layout_marginLeft="10dp"
                            android:fontFamily="@font/nanum_squareround_b"
                            />
                    </FrameLayout>
                    <ImageView
                        android:id="@+id/score_Photo"
                        android:layout_width="match_parent"
                        android:layout_height="290dp"
                        android:src="@color/gray"
                        android:scaleType="fitXY"
                        />
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:orientation="horizontal"
                        android:layout_gravity="center"
                        android:layout_marginTop="4dp">
                        <ImageView
                            android:id="@+id/Score_PicturePage1"
                            android:layout_width="4dp"
                            android:layout_height="4dp"
                            android:background="@mipmap/score_page_picture"
                            />
                        <ImageView
                            android:id="@+id/Score_PicturePage2"
                            android:layout_width="4dp"
                            android:layout_height="4dp"
                            android:layout_marginLeft="7dp"
                            android:background="@mipmap/score_page_picture"
                            />
                        <ImageView
                            android:id="@+id/Score_PicturePage3"
                            android:layout_width="4dp"
                            android:layout_height="4dp"
                            android:layout_marginLeft="7dp"
                            android:background="@mipmap/score_page_picture"
                            />
                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/score_SearchBackground"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="#99ffffff"
                        android:visibility="invisible" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:layout_margin="10dp"
                    >
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_weight="30">
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:id="@+id/score_TasteRange"
                            android:layout_marginRight="20dp"
                            >
                            <TextView
                                android:layout_width="60dp"
                                android:layout_height="wrap_content"
                                android:text="맛"
                                android:textSize="30sp"
                                android:textColor="@color/black"
                                android:layout_gravity="center|start"
                                android:layout_weight="1"
                                android:fontFamily="@font/nanum_masissneunce"
                                />
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_weight="2">
                                <ImageView
                                    android:id="@+id/score_Taste1"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Taste2"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:src="@mipmap/score_zero"
                                    android:layout_marginLeft="5dp"
                                    />
                                <ImageView
                                    android:id="@+id/score_Taste3"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Taste4"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Taste5"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.0"
                                android:textColor="@color/black"
                                android:textSize="30sp"
                                android:layout_marginLeft="20dp"
                                android:layout_gravity="center"
                                android:gravity="center"
                                android:id="@+id/score_TasteScore"
                                android:layout_weight="1"
                                android:fontFamily="@font/nanum_masissneunce"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:id="@+id/score_PriceRange"
                            android:layout_marginRight="20dp"
                            >
                            <TextView
                                android:layout_width="60dp"
                                android:layout_height="wrap_content"
                                android:text="가격"
                                android:textSize="30sp"
                                android:textColor="@color/black"
                                android:layout_gravity="center|start"
                                android:layout_weight="1"
                                android:fontFamily="@font/nanum_masissneunce"
                                />
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_weight="2">
                                <ImageView
                                    android:id="@+id/score_Price1"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Price2"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:src="@mipmap/score_zero"
                                    android:layout_marginLeft="5dp"
                                    />
                                <ImageView
                                    android:id="@+id/score_Price3"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Price4"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Price5"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.0"
                                android:textColor="@color/black"
                                android:textSize="30sp"
                                android:layout_marginLeft="20dp"
                                android:layout_gravity="center"
                                android:id="@+id/score_PriceScore"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:fontFamily="@font/nanum_masissneunce"
                                />
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:id="@+id/score_CleanRange"
                            android:layout_marginRight="20dp"
                            >
                            <TextView
                                android:layout_width="60dp"
                                android:layout_height="wrap_content"
                                android:text="청결"
                                android:textSize="30sp"
                                android:textColor="@color/black"
                                android:layout_gravity="center|start"
                                android:layout_weight="1"
                                android:fontFamily="@font/nanum_masissneunce"
                                />
                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:layout_weight="3">
                                <ImageView
                                    android:id="@+id/score_Clean1"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Clean2"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:src="@mipmap/score_zero"
                                    android:layout_marginLeft="5dp"
                                    />
                                <ImageView
                                    android:id="@+id/score_Clean3"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Clean4"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                                <ImageView
                                    android:id="@+id/score_Clean5"
                                    android:layout_width="25dp"
                                    android:layout_height="38.8dp"
                                    android:layout_marginLeft="5dp"
                                    android:src="@mipmap/score_zero"
                                    />
                            </LinearLayout>
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="0.0"
                                android:textColor="@color/black"
                                android:textSize="30sp"
                                android:layout_marginLeft="20dp"
                                android:layout_gravity="center"
                                android:id="@+id/score_CleanScore"
                                android:layout_weight="1"
                                android:gravity="center"
                                android:fontFamily="@font/nanum_masissneunce"
                                />
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_weight="1"
                        android:layout_marginRight="5dp"
                        android:layout_gravity="bottom">
                        <TextView
                            android:id="@+id/score_AverageScore"
                            android:layout_width="60dp"
                            android:layout_height="40dp"
                            android:text="0.0점"
                            android:textSize="25sp"
                            android:textColor="@color/black"
                            android:gravity="center"
                            android:background="@mipmap/average_score"
                            android:fontFamily="@font/nanum_masissneunce"
                            />
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:layout_marginTop="7dp">
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="20dp">
                    <TextView
                        android:id="@+id/score_Review"
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:text="글을 입력하세요."
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:paddingLeft="30dp"
                        android:paddingRight="30dp"
                        android:gravity="center_vertical"
                        android:background="@mipmap/review"
                        android:fontFamily="@font/nanum_squareround_l"
                        />
                    <TextView
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="30dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="나의 한줄평"
                        android:textColor="@color/black"
                        android:fontFamily="@font/nanum_squareround_l"
                        />
                </FrameLayout>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</FrameLayout>