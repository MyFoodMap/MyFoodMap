<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="5dp"
    >
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:layout_gravity="center_vertical">
        <ImageView
            android:id="@+id/itemBookmark_Img"
            android:layout_width="73dp"
            android:layout_height="20dp"
            android:src="@mipmap/spoon_select_button"
            />
        <TextView
            android:id="@+id/itemBookmark_BookmarkName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="북마크 EX 고씨네"
            android:textSize="15sp"
            android:textColor="@color/black"
            android:fontFamily="@font/nanum_squareround_eb"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="30dp"
            android:gravity="center_vertical"
            />
    </LinearLayout>
    <ImageView
        android:id="@+id/itemBookmark_Bookmark"
        android:layout_width="42dp"
        android:layout_height="31dp"
        android:src="@mipmap/bookmark_plus"
        android:layout_gravity="end"
        />
</FrameLayout>