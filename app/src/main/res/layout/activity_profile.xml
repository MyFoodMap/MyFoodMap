<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".ProfileActivity"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="20dp">
        <ImageView
            android:id="@+id/profile_BasicProfile"
            android:layout_width="130dp"
            android:layout_height="131dp"
            android:src="@mipmap/basic_profile"
            android:layout_weight="2"
            />
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_weight="1"
            >
            <FrameLayout
                android:layout_width="192dp"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="22dp">
                <ImageView
                    android:id="@+id/profile_Setting_Button"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/setting"
                    />
                <ImageView
                    android:id="@+id/profile_Logout_Button"
                    android:layout_width="19dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/logout"
                    android:layout_gravity="end"
                    />
            </FrameLayout>
            <TextView
                android:id="@+id/profile_UserName"
                android:layout_width="192dp"
                android:layout_height="40dp"
                android:background="@mipmap/user_name"
                android:layout_marginTop="10dp"
                android:text="닉네임"
                android:textSize="20sp"
                android:fontFamily="@font/nanum_squareround_l"
                android:textColor="@color/black"
                android:gravity="center_vertical"
                android:paddingLeft="10dp"
                />
            <TextView
                android:id="@+id/profile_ProfileEdit_Button"
                android:layout_width="190dp"
                android:layout_height="20dp"
                android:background="@mipmap/profile_edit"
                android:layout_marginTop="18dp"
                android:text="프로필 편집"
                android:textSize="10sp"
                android:textColor="@color/black"
                android:gravity="center"
                android:fontFamily="@font/nanum_squareround_l"
                />
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp">
        <FrameLayout
            android:id="@+id/profile_PeedClickRange"
            android:layout_width="180dp"
            android:layout_height="43dp">
            <ImageView
                android:layout_width="180dp"
                android:layout_height="43dp"
                android:src="@mipmap/click_range"
                />
            <ImageView
                android:id="@+id/profile_PeedIcon_Button"
                android:layout_width="30dp"
                android:layout_height="32.42dp"
                android:src="@mipmap/peed_icon"
                android:layout_gravity="center"
                />
        </FrameLayout>
        <FrameLayout
            android:id="@+id/profile_BookmarkClickRange"
            android:layout_width="180dp"
            android:layout_height="43dp">
            <ImageView
                android:layout_width="180dp"
                android:layout_height="43dp"
                android:src="@mipmap/click_range"
                />
            <ImageView
                android:id="@+id/profile_BookmarkIcon_Button"
                android:layout_width="20dp"
                android:layout_height="24.64dp"
                android:src="@mipmap/bookmark_icon"
                android:layout_gravity="center"
                />
        </FrameLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageView
            android:id="@+id/profile_SelectPeed"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:src="@mipmap/select_peed_bookmark"
            android:layout_weight="1"
            />
        <ImageView
            android:id="@+id/profile_SelectBookmark"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:src="@mipmap/select_peed_bookmark"
            android:layout_weight="1"
            android:visibility="invisible"
            />
    </LinearLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:id="@+id/profile_PeedPage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/profile_PlaceCitySelect_Button"
                    android:layout_width="65dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/place_city_select_button"
                    android:layout_weight="1"
                    />
                <ImageView
                    android:id="@+id/profile_PlaceDongSelect_Button"
                    android:layout_width="65dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/place_dong_select_button"
                    android:layout_weight="1"
                    />
                <ImageView
                    android:id="@+id/profile_PlaceGuSelect_Button"
                    android:layout_width="65dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/place_gu_select_button"
                    android:layout_weight="1"
                    />
                <ImageView
                    android:id="@+id/profile_PlaceNameSearch_Button"
                    android:layout_width="146dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/place_name_search_button"
                    android:layout_weight="2"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_margin="5dp">
                <ImageView
                    android:id="@+id/profile_PeedPicture"
                    android:layout_width="118dp"
                    android:layout_height="118dp"
                    android:src="@mipmap/peed_picture"
                    />
                <TextView
                    android:id="@+id/profile_PeedName"
                    android:layout_width="118dp"
                    android:layout_height="20dp"
                    android:background="@mipmap/peed_place_name"
                    android:text="가게이름"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nanum_squareround_l"
                    android:textSize="10sp"
                    android:gravity="center_vertical"
                    android:paddingLeft="5dp"
                    />
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerViewGallery"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>
        <FrameLayout
            android:id="@+id/profile_BookmarkPage"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="10dp"
            android:visibility="invisible"
            >
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_gravity="center_vertical">
                <ImageView
                    android:id="@+id/profile_SpoonSelect_Button"
                    android:layout_width="73dp"
                    android:layout_height="20dp"
                    android:src="@mipmap/spoon_select_button"
                    android:layout_gravity="center_vertical"
                    />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="북마크 EX 고씨네"
                    android:textSize="20sp"
                    android:textColor="@color/black"
                    android:fontFamily="@font/nanum_squareround_eb"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="30dp"
                    />
            </LinearLayout>
            <ImageView
                android:id="@+id/profile_BookmarkPlus"
                android:layout_width="42dp"
                android:layout_height="31dp"
                android:src="@mipmap/bookmark_plus"
                android:layout_gravity="end"
                />
            <ImageView
                android:id="@+id/profile_BookmarkNo"
                android:layout_width="42dp"
                android:layout_height="31dp"
                android:src="@mipmap/bookmark_no"
                android:layout_gravity="end"
                android:visibility="invisible"
                />
        </FrameLayout>
    </FrameLayout>
</LinearLayout>